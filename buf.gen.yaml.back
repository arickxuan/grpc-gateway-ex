version: v1
plugins:
  - name: go
    out: gen
    opt: paths=source_relative
  - name: go-grpc
    out: gen
    opt:
      - paths=source_relative
      - require_unimplemented_servers=false
  - name: grpc-gateway
    out: gen
    opt: paths=source_relative
  - name: openapiv2
    out: gen/docs
  - name: validate
    out: gen
    opt:
      - lang=go


